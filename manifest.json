{
  "name": "Shell In A Box",
  "id": "shellinabox",
  "description": {
    "en": "Web based AJAX terminal emulator",
    "fr": "Ã‰mulateur de terminal web"
  },
  "url": "https://code.google.com/p/shellinabox/",
  "maintainer": {
    "name": "kload",
    "email": "kload@kload.fr"
  },
  "multi_instance": "false",
  "arguments": {
    "install": [
      {
        "name": "domain",
        "type": "domain",
        "ask": {
          "en": "Choose a domain for Shell In A Box"
        },
        "example": "domain.org"
      },
      {
        "name": "path",
        "type": "path",
        "ask": {
          "en": "Choose a path for Shell In A Box"
        },
        "example": "/ssh",
        "default": "/ssh"
      }
    ]
  }
}
